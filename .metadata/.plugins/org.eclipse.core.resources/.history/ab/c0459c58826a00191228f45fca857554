package com.syzy.forbean;

public class Fordemo {
/*
 *               88
 *          888888888888
 *         88888888888888
 *      88888888888888888888
 *         88888888888888      
 *            88888888
 *               88
 * 
 * 
 * 
 * */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	for(int i=1;i<=24;i++){
		for(int x=i;x<=10;x++){
			System.out.print(" ");
		}
		for(int y=1;y<=i;y++){
			System.out.print("*");
		}
		for(int y=1;y<=i;y++){
			System.out.print("*");
		}
		System.out.println();
	}
	
	
	}}


